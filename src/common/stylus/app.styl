.box-flex
  display flex

.align-center
  align-items center

.flex
  flex 1 1 auto

.ui-flex
  display -webkit-box
  display flex

.image-icons
  background-color transparent
  background-repeat no-repeat
  background-position 50%
  background-size cover
  display: inline-block;

a
  text-decoration none
  -webkit-tap-highlight-color rgba(0,0,0,0)
  background-color transparent
  outline 0

a:link
  text-decoration none

a:active
  text-decoration none

*
  background-repeat no-repeat
  padding 0
  margin 0
  box-sizing border-box
:after, :before
  text-decoration: inherit;
  vertical-align: inherit;

.justify-start
  -webkit-box-pack start
  justify-content flex-start
.justify-space-between
  -webkit-box-pack: justify;
  justify-content: space-between;
.fz-xl
  font-size 24px !important

.fz-xs
  font-size 14px !important
.fz-xxs
  font-size: 12px !important;
.fsc1
  font-size: 12px;
  color: rgba(0,0,0,.54);
.align-end
  -webkit-box-align: end;
  align-items: flex-end;
.layout
  display: -webkit-box;
  display: flex;
  flex-wrap: nowrap;
.bd-left-1px:before,
.bd-right-1px:before,
.bd-left-1px:after,
.bd-right-1px:after
  content: "";
  display: block;
  position: absolute;
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;

.bd-left-1px:before
  border-left: 1px solid #d9d9d9;
  top: 0;
  left: 0;
  height: 100%;
  -webkit-transform-origin: left 0;
  transform-origin: left 0;
  transform: scaleX(.5)
.border-bottom-1px:before
  height: 1px;
  content: "";
  width: 100%;
  border-top: 1px solid #999;
  position: absolute;
  top: 100%;
  right: 0;
  transform: scaleY(.5);
  -webkit-transform: scaleY(.5);
.bd-left-1px
.bd-right-1px
.bd-top-1px
.bd-bottom-1px
.border-1px
.border-top-1px
  position: relative;
.bd-right-1px
  border-right: 1px solid #d9d9d9;
  top: 0;
  right: 0;
  height: 100%;
  -webkit-transform-origin: right 0;
  transform-origin: right 0;
.border-top-1px:before
  height: 1px;
  content: "";
  width: 100%;
  border-top: 1px solid #999;
  position: absolute;
  top: -1px;
  right: 0;
  transform: scaleY(.5);
  -webkit-transform: scaleY(.5);
.border-1px:before
  content: "";
  pointer-events: none;
  box-sizing: border-box;
  position: absolute;
  width: 200%;
  height: 200%;
  left: 0;
  top: 0;
  border-radius: 0;
  border: 1px solid #999;
  -webkit-transform: scale(.5);
  -webkit-transform-origin: 0 0;
  transform: scale(.5);
  transform-origin: 0 0;
.border-1px.card-box:before
  border-color: #e5e5e5;
  border-radius: 10px;
.fill-height
  height 100%
.more:after
  content: "";
  position: absolute;
  right: 20px;
  top: 50%;
  width: 12px;
  height: 12px;
  border-left: 1px solid currentColor;
  border-top: 1px solid currentColor;
  transform: translate3d(0,-50%,0) rotate(135deg);
  -webkit-transform: translate3d(0,-50%,0) rotate(135deg);

header
  height: 50px;
  background: transparent;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  z-index: 10;
.padding-8-15
  padding 8px 15px !important
.app-view-wrapper
  position: relative;
  margin: 0 auto;
.justify-center
  -webkit-box-pack: center;
  justify-content: center;
.align-start
  -webkit-box-align: start;
  align-items: flex-start;
.layout.wrap
  flex-wrap: wrap;
.layout.row
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  flex-direction: row;
.align-content-start
  align-content: flex-start;
.btn
  display: block;
  outline: 0;
  background: #ff6700;
  color: #fff;
  text-align: center;
  width: 100%;
  height: 60px;
  line-height: 60px;
  font-size: 18px;
.ui-line
  height 12px
  background #f5f5f5
  overflow hidden
  clear both
.page-wrap
  height: 100%;
  overflow-x: hidden;
  overflow-y: auto;
